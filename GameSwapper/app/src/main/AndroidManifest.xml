<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gameswapper">

    <application

        android:allowBackup="true"
        android:icon="@mipmap/gc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="com.samsung.android.icon_container.has_icon_container" android:value="true"/>
        <activity android:name=".UserProfileActivity" android:theme="@style/AppThemeDark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddGamesToProfileActivity" android:theme="@style/AppThemeDark"/>
        <activity android:name=".SignupActivity" android:theme="@style/AppThemeDark" />
        <activity android:name=".LoginActivity" android:theme="@style/AppThemeDark" />
        <activity android:name=".GamesOnMarketActivity" android:theme="@style/AppThemeDark" />
        <activity android:name=".MyGamesActivity" android:theme="@style/AppThemeDark" />
    </application>

</manifest>